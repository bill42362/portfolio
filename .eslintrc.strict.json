{
  "extends": "./.eslintrc.json",
  "rules": {
    "no-debugger": ["error"],
    "no-console": ["error"],
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single", { "avoidEscape": true }],
    "semi": ["error", "always"],
    "prettier/prettier": "error",
    "react/no-unused-prop-types": ["error"]
  }
}
